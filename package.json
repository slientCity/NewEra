{
  "name": "ng-matero",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:release": "ng build --prod --baseHref=/ng-matero/",
    "build:schematics": "npm run copy:schematics && cd schematics && npm run build && cd .. && npm run build:starter",
    "build:starter": "gulp --gulpfile gulpfile.js",
    "copy:schematics": "npm run clean:schematics && cpr schematics dist/schematics",
    "clean:schematics": "rimraf dist/schematics",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "tslint -p tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:scss": "stylelint --syntax scss 'src/**/*.scss' --fix",
    "hmr": "ng serve --hmr -c hmr --disable-host-check",
    "deploy": "ng deploy",
    "release": "npm run build:release && npm run deploy",
    "publish": "npm run build:schematics && cd dist/schematics && npm publish",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.4",
    "@angular/cdk": "~10.2.7",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.2.4",
    "@angular/localize": "~10.2.4",
    "@angular/material": "~10.2.7",
    "@angular/material-moment-adapter": "~10.2.7",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "@mat-datetimepicker/core": "^5.1.2",
    "@mat-datetimepicker/moment": "^5.1.2",
    "@ng-matero/extensions": "^10.13.1",
    "@ng-select/ng-option-highlight": "0.0.6",
    "@ng-select/ng-select": "^5.1.0",
    "@ngx-formly/core": "^5.10.11",
    "@ngx-formly/material": "^5.10.11",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "apexcharts": "^3.23.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "ngx-permissions": "^8.1.1",
    "ngx-progressbar": "^6.1.0",
    "ngx-toastr": "^13.1.0",
    "photoviewer": "^3.5.4",
    "rxjs": "~6.6.0",
    "screenfull": "^5.1.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular/cli": "~10.2.1",
    "@angular/compiler-cli": "~10.2.4",
    "@angular/language-service": "~10.2.4",
    "@angularclass/hmr": "^3.0.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/parse5": "~5.0.2",
    "angular-cli-ghpages": "^0.6.2",
    "chalk": "^3.0.0",
    "codelyzer": "^6.0.0",
    "cpr": "^3.0.1",
    "gulp": "^4.0.2",
    "gulp-each": "^0.5.0",
    "husky": "^4.3.7",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.3",
    "parse5": "^5.1.1",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
